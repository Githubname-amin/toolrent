<!--components/foot-bar/foot-bar.wxml-->
<view class="footBar">
    <view wx:for="{{footMap}}" wx:key="index" class="footBarBox" bind:tap="handleFootbar" data-footIndex="{{item.index}}">
        <image class="footBarImg" src="/img/{{item.imgSrc}}.png"></image>
        <view>
            {{item.message}}
        </view>
    </view>
</view>